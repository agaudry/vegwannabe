{
	"name": "client",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint"
	},
	"dependencies": {
		"axios": "^0.21.1",
		"core-js": "^3.6.5",
		"tailwindcss": "^1.8.13",
		"vue": "^2.6.12",
		"vue-router": "^3.4.6"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "^4.5.7",
		"@vue/cli-plugin-eslint": "^4.5.7",
		"@vue/cli-service": "^4.5.7",
		"@vue/eslint-config-airbnb": "^5.1.0",
		"babel-eslint": "^10.1.0",
		"eslint": "^6.7.2",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-vue": "^6.2.2",
		"vue-template-compiler": "^2.6.12"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"@vue/airbnb"
		],
		"parserOptions": {
			"parser": "babel-eslint"
		},
		"rules": {
			"semi": "off",
			"comma-dangle": "off",
			"max-len": "off",
			"quote-props": 1,
			"prefer-destructuring":"warn"
		}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions"
	]
}
